// ウィンドウIDの確認
count = GETALLWIN()
PRINT "総ウィンドウ数: " + count

FOR i = 0 TO count - 1
    id = ALL_WIN_ID[i]
    IF id > 0 THEN
        title = STATUS(id, ST_TITLE)
        clsname = STATUS(id, ST_CLASS)
        IF POS("PowerShell", title) > 0 OR POS("Console", clsname) > 0 THEN
            PRINT "PowerShell系ウィンドウ発見:"
            PRINT "ID=" + id + " タイトル=" + title + " クラス=" + clsname
        ENDIF
    ENDIF
NEXT