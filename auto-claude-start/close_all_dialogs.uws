// 全てのエラーダイアログを閉じる

// 方法1: ダイアログウィンドウを探して閉じる
FOR i = 0 TO 20
    // ダイアログクラス名 #32770 を探す
    dlg = GETID("", "#32770", i)
    IF dlg = -1 THEN BREAK
    IF dlg > 0 THEN
        // OKボタンをクリック
        CLKITEM(dlg, "OK", CLK_BTN)
        SLEEP(0.2)
    ENDIF
NEXT

// 最終確認のスクリーンショット
SLEEP(1)
SAVEIMG("C:\Users\user\claude-test-100-v0.2\auto-claude-start\all_dialogs_closed.jpg", 0, , , , , , 80)