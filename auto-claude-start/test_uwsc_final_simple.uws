// 最終版 - シンプルなウィンドウ一覧とcmd起動

// ログファイルを開く
fid = FOPEN("C:\Users\user\claude-test-100-v0.2\auto-claude-start\uwsc_final_log.txt", F_WRITE)

// 起動前
FPUT(fid, "=== Before ===")
win_count = GETALLWIN()
FPUT(fid, "Total windows: " + win_count)

// cmd.exeを起動してスクリーンショット
EXEC("cmd.exe")
SLEEP(3)
SAVEIMG("C:\Users\user\claude-test-100-v0.2\auto-claude-start\uwsc_with_new_cmd.jpg", 0, , , , , , 70)

// 起動後
FPUT(fid, "")
FPUT(fid, "=== After ===")
win_count2 = GETALLWIN()
FPUT(fid, "Total windows: " + win_count2)
FPUT(fid, "Difference: " + (win_count2 - win_count))

// ファイルを閉じる
FCLOSE(fid)