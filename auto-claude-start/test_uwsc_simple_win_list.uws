// シンプルなウィンドウ一覧取得（STATUS関数を使わない）

// ログファイルを開く
fid = FOPEN("C:\Users\user\claude-test-100-v0.2\auto-claude-start\uwsc_simple_win_list.txt", F_WRITE)

// 起動前のウィンドウ数を数える
FPUT(fid, "=== 起動前 ===")
win_count_before = GETALLWIN()
FPUT(fid, "総ウィンドウ数: " + win_count_before)

// cmd.exeを起動
FPUT(fid, "")
FPUT(fid, "=== cmd.exe起動 ===")
EXEC("cmd.exe")
SLEEP(3)

// 起動後のウィンドウ数を数える
FPUT(fid, "")
FPUT(fid, "=== 起動後 ===")
win_count_after = GETALLWIN()
FPUT(fid, "総ウィンドウ数: " + win_count_after)
FPUT(fid, "増加数: " + (win_count_after - win_count_before))

// ファイルを閉じる
FCLOSE(fid)

// 完了のスクリーンショット
SAVEIMG("C:\Users\user\claude-test-100-v0.2\auto-claude-start\uwsc_simple_complete.jpg", 0, , , , , , 70)