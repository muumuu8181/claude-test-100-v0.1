// 最小限のウィンドウ一覧取得

// ファイルを書き込みモードで開く
fid = FOPEN("C:\Users\user\claude-test-100-v0.2\auto-claude-start\uwsc_simple_log.txt", F_WRITE)

// 現在のウィンドウ数を数える
count = 0
FOR i = 0 TO 50
    hwnd = GETID("", "", i)
    IF hwnd = -1 THEN BREAK
    IF STATUS(hwnd, ST_VISIBLE) THEN
        count = count + 1
    ENDIF
NEXT

FPUT(fid, "ウィンドウ数: " + count)

// cmd.exeを起動
cmd_pid = EXEC("cmd.exe")
FPUT(fid, "cmd.exe PID: " + cmd_pid)

// ファイルを閉じる
FCLOSE(fid)